  <sql id="queryCondition">
    <where>
      <if test="orderId != null and orderId !='' ">
        AND order_id = #{orderId,jdbcType=VARCHAR}
      </if>
      <if test="orderIdList != null">
        AND order_id in
        <foreach item="item" collection="orderIdList" open="(" separator=","
          close=")">
          #{item}
        </foreach>
      </if>
      <if test="ebpCode != null and ebpCode !='' ">
        AND ebp_code = #{ebpCode,jdbcType=VARCHAR}
      </if>
      <if test="ebcCode != null and ebcCode !='' ">
        AND ebc_code = #{ebcCode,jdbcType=VARCHAR}
      </if>
      <if test="tpl != null and tpl != '' ">
        AND tpl = #{tpl,jdbcType=VARCHAR}
      </if>
      <if test="deliveryCode != null and deliveryCode !='' ">
        AND delivery_code = #{deliveryCode,jdbcType=VARCHAR}
      </if>
      <if test="deliveryCodeList != null">
        AND delivery_code in
        <foreach item="item" collection="deliveryCodeList" open="(" separator=","
          close=")">
          #{item}
        </foreach>
      </if>
      <if test="customsType != null">
        AND customs_type = #{customsType,jdbcType=INTEGER}
      </if>
      <if test="customsCode != null">
        AND customs_code = #{customsCode,jdbcType=VARCHAR}
      </if>
      <if test="createTimeStart!=null">
        AND create_time <![CDATA[ >= ]]> #{createTimeStart,jdbcType=TIMESTAMP}
      </if>
      <if test="createTimeEnd!=null">
        AND create_time <![CDATA[ <= ]]> #{createTimeEnd,jdbcType=TIMESTAMP}
      </if>
      <if test="orderDateStart!=null">
        AND order_date <![CDATA[ >= ]]> #{orderDateStart,jdbcType=TIMESTAMP}
      </if>
      <if test="orderDateEnd!=null">
        AND order_date <![CDATA[ <= ]]> #{orderDateEnd,jdbcType=TIMESTAMP}
      </if>
    </where>
  </sql>

